<div class="col-xs-12 thumbnail">
  <h2> <%= user.name %> </h2>
  <p> <%= user.group_id %>
  <%= link_to 'Show', user, class: "btn btn-primary btn-lg btn-block" %>
  <%= link_to 'Edit', edit_user_path(user), class: "btn btn-warning btn-lg btn-block" %>
  <%= link_to 'Delete', user, method: :delete, class: "btn btn-danger btn-lg btn-block" %>
</div>
